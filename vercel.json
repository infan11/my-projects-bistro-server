{
    "version": 2,
    "builds": [
      {
        "src": "path/to/your/source",
        "use": "@vercel/static-build",
        "config": { "foo": "bar" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  